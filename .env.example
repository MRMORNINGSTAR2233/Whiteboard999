# Groq AI API Key
# Get your API key from: https://console.groq.com/keys
GROQ_API_KEY=

# Optional: Set the AI model to use
GROQ_MODEL=llama-3.3-70b-versatile

# Application Settings
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Database Configuration
DATABASE_URL=

# NextAuth Configuration
NEXTAUTH_SECRET=
NEXTAUTH_URL=http://localhost:3000

# Prisma Logging Configuration
# Options: 'default' (warn/error only), 'verbose' (includes queries), 'debug' (all logs)
PRISMA_LOG_LEVEL=default

# Email Configuration (Resend)
# Get your API key from: https://resend.com/api-keys
RESEND_API_KEY=
EMAIL_FROM=Whiteboard <noreply@yourdomain.com>

# OAuth Providers (Optional)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GITHUB_ID=
GITHUB_SECRET=

# Pusher Configuration (for real-time collaboration)
NEXT_PUBLIC_PUSHER_APP_KEY=
PUSHER_APP_ID=
PUSHER_SECRET=
NEXT_PUBLIC_PUSHER_CLUSTER=
